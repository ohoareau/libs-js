{
  "name": "@ohoareau/gatsby-source-graphql-custom",
  "description": "Gatsby plugin which adds a third-party GraphQL API to Gatsby GraphQL",
  "version": "0.3.0",
  "license": "MIT",
  "main": "index.js",
  "types": "index.d.ts",
  "directories": {
    "lib": ".",
    "test": "__tests__"
  },
  "files": [
    "*.js",
    "utils"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@graphql-tools/links": "v6.0.9",
    "@graphql-tools/utils": "v6.0.9",
    "@graphql-tools/wrap": "v6.0.9",
    "apollo-link": "1.2.14",
    "apollo-link-http": "^1.5.17",
    "dataloader": "^2.0.0",
    "graphql": "^14.6.0",
    "invariant": "^2.2.4",
    "node-fetch": "^2.6.1",
    "uuid": "^8.2.0"
  },
  "peerDependencies": {
    "gatsby": "^2.0.15"
  },
  "devDependencies": {
    "gatsby": "^2.0.15"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ohoareau/libs-js.git",
    "directory": "packages/gatsby-source-graphql-custom"
  },
  "scripts": {
    "clean": "rm -f gatsby-*.* && rm -f index.* && rm -rf utils/",
    "build": "tsc",
    "test": "../../node_modules/.bin/jest -c ../../jest.config.js --rootDir=`pwd`"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin"
  ]
}
